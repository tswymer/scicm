{
  "name": "icm",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI (ESM)",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/Development/icm",
  "bugs": "https://github.com/Development/icm/issues",
  "repository": "Development/icm",
  "license": "MIT",
  "author": "Tobias Wymer",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "icm": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "predev": "npm run build && clear",
    "dev": "node ./bin/run.js",
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "icm",
    "commands": "./dist/commands",
    "dirname": "icm",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@inquirer/prompts": "^4.1.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "@sap-cloud-sdk/odata-v2": "^3.12.0",
    "@sap-cloud-sdk/util": "^3.12.0",
    "moment": "^2.30.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@sap-cloud-sdk/generator": "^3.12.0",
    "@types/node": "^18",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "oclif": "^4.4.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
